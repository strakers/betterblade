{
    "name": "strakez/betterblade",
    "description": "A port of Laravel 9's Blade::render functionality for previous versions",
    "type": "library",
    "keywords": ["Laravel", "Blade", "Render", "Inline"],
    "authors": [
        {
            "name": "Steven Straker",
            "email": "contact@strakez.com"
        }
    ],
    "require": {
        "php" : ">=7.1",
        "illuminate/container": "^7.0|^8.0",
        "illuminate/contracts": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/view": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "BetterBlade\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BetterBlade\\BetterBladeServiceProvider"
            ],
            "aliases": {
                "BetterBlade": "BetterBlade\\BetterBladeFacade"
            }
        }
    }
}
